{
  "name": "template-gatsby-typescript",
  "version": "1.0.0",
  "repository": "https://github.com/constrod/template-gatsby-typescript",
  "bugs": {
    "url": "https://github.com/constrod/template-gatsby-typescript/issues"
  },
  "contributors": [
    {
      "name": "Rodrick Alcantara",
      "url": "https://github.com/constROD"
    }
  ],
  "description": "Custom Template for Gatsby TypeScript",
  "author": "Rodrick Alcantara <rialcantara21@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "eslint src --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint src --fix --ext .js,.ts,.jsx,.tsx",
    "pre-commit": "lint-staged --allow-empty",
    "prepare": "husky install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.1",
    "babel-plugin-styled-components": "^2.0.2",
    "gatsby": "^4.6.0",
    "gatsby-plugin-styled-components": "^5.6.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.6",
    "react-use": "^17.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.14",
    "@types/redux-logger": "^3.0.9",
    "@types/styled-components": "^5.1.21",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-jest": "^27.4.6",
    "babel-preset-gatsby": "^2.6.0",
    "eslint": "8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "gatsby-plugin-root-import": "^2.0.8",
    "husky": "6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.2",
    "lodash": "^4.17.21",
    "node-sass": "^7.0.1",
    "prettier": "^2.5.1",
    "redux-logger": "^3.0.6",
    "styled-components": "^5.3.3",
    "typescript": "^4.5.5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix"
    ]
  }
}
